class Sprite {
 
  PVector position;
  String name;
  float diameter, radius;
  
  //**************************************************
  public String getName(){
    return this.name;
  }
  
  //**************************************************
  public float diameter(){
    return this.diameter;
  }  
  
  //**************************************************
  public float getRadius(){
    return this.radius;
  }   
  
  //**************************************************
  public PVector position(){
    return this.position;
  }
  
}
